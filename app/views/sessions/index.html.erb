  <div class="container">
  <div class="register">
      
    
      <% if flash[:notice]%>
      <%= flash[:notice]%>
      <% end %>
    
  <h1>Register Here:</h1>
  <form class="register" action="/create" method="post">
  <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
  <p> <label>First Name:
  <input type="text" class="form-control" name="first_name"></label> </p>
  <p> <label>Last Name:
  <input type="text" class="form-control" name="last_name"></label> </p>
  <p> <label>Email:
  <input type="text" class="form-control" name="email"></label> </p>
  <p> <label>Password:
  <input type="text" class="form-control" name="password"></label> </p>
  <p> <label>Shoe Size:
  <input type="number" class="form-control" name="shoe_size"></label> </p>
  <p> <label>Gender
  <input type="text" class="form-control" name="gender"></label> </p>
  <input type="submit" class="btn btn-primary" value="Register">
</form>
  </div>
<div class="login">
  <h1>Login Here:</h1>
  <form action="/login" method="post">
  <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
  <div class="form-group">
    
    <label>Email: <input type="text" name="email" class="form-control"></label>
  </div>
  <div class="form-group">
    
    <label>Password: <input type="text" class="form-control" name="password"></label>
  </div>
  <input class="btn btn-primary" type="submit" value="Submit">
</form>
</div>
</div>





